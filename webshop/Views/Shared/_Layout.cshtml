    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const themeToggleButton = document.getElementById('bd-theme');
            const themeToggleButton2 = document.getElementById('bd-theme-mobil');
            const themeValue = localStorage.getItem('theme') || 'light';

            document.documentElement.setAttribute('data-bs-theme', themeValue);

            const updateThemeIcon = (theme) => {
                const icon = theme === 'dark' ? '#moon-stars-fill' : '#sun-fill';

                document.querySelector('#bd-theme svg use').setAttribute('href', icon);
                if (themeToggleButton2) {
                    document.querySelector('#bd-theme-mobil svg use').setAttribute('href', icon);
                }
            };

            updateThemeIcon(themeValue);

            function toggleTheme() {
                let newTheme = document.documentElement.getAttribute('data-bs-theme') === 'dark' ? 'light' : 'dark';
                document.documentElement.setAttribute('data-bs-theme', newTheme);
                localStorage.setItem('theme', newTheme);
                updateThemeIcon(newTheme);
            }

            themeToggleButton.addEventListener('click', toggleTheme);
            if (themeToggleButton2) {
                themeToggleButton2.addEventListener('click', toggleTheme);
            }

            document.querySelectorAll('[data-bs-theme-value]').forEach(button => {
                button.addEventListener('click', function () {
                    let theme = this.getAttribute('data-bs-theme-value');
                    document.documentElement.setAttribute('data-bs-theme', theme);
                    localStorage.setItem('theme', theme);
                    updateThemeIcon(theme);
                });
            });
        });

    </script>



